        }

        const doc = await getDoc(docId);
        

        axios.get(`/api/organization/${doc.organization_id}`).then((getOrg) => {

            const org = getOrg.data;
            const metadata = org.metadata;

            $(`.workspace`).css('background-color', `${metadata.config.dados.corDestaque}`);


            if(metadata.config.dados.fotoEmpresa != ""){

                $(`#logo`).attr('src', `/img/fotos/${metadata.config.dados.fotoEmpresa}`).fadeIn(200);

                $(`svg.logo`).remove();
            }

        }).catch((error) => {
            alert(error)
        })

        var signers = doc.signers;
        var assinados = 0;

        for(let i = 0; i < signers.length; i++){
            const element = signers[i];

            if(element.hash != "" && element.email == signer.email){
                assinados++;
            }
        }

        if(doc.status == "deleted"){
            setStep('expirado');
            setTimeout(()=>{
                $('.waiting').fadeOut(200);
            },1000);
        }else if(assinados > 0){
            {{!-- setStep('documento'); --}}
            setStep('obrigado');
            setTimeout(()=>{
                $('.waiting').fadeOut(200);
            },1000);
        }else{
            setStep('termos');
            setTimeout(()=>{
                $('.waiting').fadeOut(200);
            },1000)
        }


        async function checkPowerUps(pipe_id, card){

            var getPipe = await axios.get(`/api/pipe/${pipe_id}`);

            if(getPipe.status != 200){
                alert('checkPowerUps informa: '+getPipe.data);
                return;
            }

            var pipe = getPipe.data;

            if (pipe.metadata.powerups) {

                var powers = pipe.metadata.powerups.filter(item => item.condition.id == "proposta_aceita");

                if (powers.length > 0) {

                    for (power of powers) {
                        activePowerUp(card, power);
                    }

                }

            } 
        }

    }

    renderDoc(docId);

</script>