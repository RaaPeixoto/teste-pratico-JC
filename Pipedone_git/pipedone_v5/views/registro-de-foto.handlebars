    <div class="conteudo">Carregando...</div>

</section>

<script src="/js/registro-de-foto.js"></script>
<script>
    const registro = JSON.parse('{{{meta}}}');
    console.log(registro)
    const creator_id = registro.creator_id;

    axios.get(`/api/organization/${registro.organization_id}`).then((getOrg) => {

        const org = getOrg.data;

        const metadata = org.metadata;

        $(`.workspace`).css('background-color', `${metadata.config.dados.corDestaque}`);

        if(metadata.config.dados.fotoEmpresa != ""){

            $(`#logo`).attr('src', `/img/fotos/${metadata.config.dados.fotoEmpresa}`).fadeIn(200);

            $(`svg.logo`).remove();
        }

    }).catch((error) => {
        console.log(error)
    })


    function setStep(step) {
        $('.conteudo').load(`/html/registro-fotos/${step}.html`, () => {
            $('.conteudo').fadeIn(200);
        })
    };

    function setButtons(text, numberButtons, nextStep, backStep) {

        $(`.conteudo`).append(`<section class="botoes"></section>`)
        $(`.botoes`).html('');

        var back = `<button class="back" onclick="setStep('${backStep}')">Voltar</button>`;
        var next = `<button class="next" onclick="setStep('${nextStep}')">${text}</button>`;

        if (numberButtons == 2) {
            $(`.botoes`).append(back)
            $(`.botoes`).append(next)
        } else {
            $(`.botoes`).append(next)
        }
        
    }

    setStep('termos');
    {{!-- setStep('obrigado'); --}}

</script>
