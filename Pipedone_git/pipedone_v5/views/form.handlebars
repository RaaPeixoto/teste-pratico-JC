<script>

const doc = JSON.parse('{{{form}}}');
const meta = doc.metadata;
const form = doc.metadata.form;
const pipe_id = doc.id;
const rastreador = '{{rastreador}}';

axios.get(`/api/organization/${doc.organization_id}`).then((getOrg) => {

    const org = getOrg.data;
    const metadata = org.metadata;

    $(`.workspace`).css('background-color', `${metadata.config.dados.corDestaque}`);


    if(metadata.config.dados.fotoEmpresa != ""){

        $(`#logo`).attr('src', `/img/fotos/${metadata.config.dados.fotoEmpresa}`).fadeIn(200);

        $(`svg.logo`).remove();
    }

}).catch((error) => {
    alert(error)
})


function generatorElementByType(element) {

    var elemento = "<div class='container'>";

    switch (element.type) {
        
        case 'title':
            elemento += `<h1>${element.description}</h1>`;
            break;

        case 'subTitle':
            elemento += `<p>${element.description}</p>`;
            break;

        case 'breakLine':
            elemento += `<hr>`;
            break;

        case 'text':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input type="text" class="dados" value="" placeholder="Digite aqui..." id="${element.id}">`;
            break;

        case 'file':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input type="file" class="dados" id="${element.id}">`;
            break;

        case 'date':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input value="" class="dados"  style="width: 172px;" type="date" id="${element.id}">`;
            break;

        case 'year':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input placeholder="0000" class="dados"  style="width: 85px;" type="tel"  onkeypress="$(this).mask('0000')"  max="4" id="${element.id}">`;
            break;

        case 'phone':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input class="dados"  placeholder="(00) 0 0000-0000"  style="width: 180px;" onkeypress="$(this).mask('(00) 0 0000-0000')" type="tel" id="${element.id}">`;
            break;

        case 'cpf':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input class="dados"  placeholder="000.000.000-00" style="width: 172px;"  onkeypress="$(this).mask('000.000.000-00')"  type="tel" id="${element.id}">`;
            break;

        case 'placa':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input class="dados"  placeholder="AAA-0A00" style="width: 172px;"  onkeypress="$(this).mask('AAA-0A00')"  type="text" id="${element.id}">`;
            break;

        case 'time':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<input class="dados"  type="time" style="width: 120px;" id="${element.id}">`;
            break;

        case 'textarea':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            elemento += `<textarea class="dados"  placeholder="Digite aqui..." id="${element.id}"></textarea>`;
            break;

        case 'radio':

            elemento += `<h4 class="titulo">${element.description}</h4>`;

            for (campo of element.meta.campos) {

                elemento += `<label><input class="dados" type="radio" name="${element.id}"  id="${element.id}"  value="${campo.id}">${campo.option}</label>`;

            }

            break;

        case 'checkbox':

            elemento += `<h4 class="titulo">${element.description}</h4>`;
            for (campo of element.meta.campos) {

                elemento += `<label><input class="dados" type="checkbox" id="${campo.id}" value="${campo.id}">${campo.option}</label>`;

            }

            break;

    }

    elemento += '</div>';

    return elemento;

}

var campos = "";
for(campo of form){

    campos += generatorElementByType(campo);

}

campos += `<div class='container'><label><input type="checkbox" id="permissao"> Concordo em enviar os dados acima para a empresa solicitante através do Pipedone.</label></div>`;

campos += `<div class='container'><button id="set-dados" onclick="setDadosCard()">Enviar dados</button></div>`;

$('.conteudo').html(campos);
$('.waiting').fadeOut(200);
$('.workspace').fadeIn(200);


function newId() {
    return Math.floor(Date.now() * Math.random()).toString(36);
}

async function setDadosCard(){

    $('#set-dados').text('Aguarde...')

    var permissao = $('#permissao');

    if(permissao[0].checked == false){
        error('Você precisa concordar com o envio dos dados.');
        return;
    }

    var buscaSteps = await axios.get(`/api/step/public/listar/${pipe_id}`);
    var step_id = buscaSteps.data[0].id;

    var campos = $('.dados')

    var formulario = {
        fields: form,
        values: []
    }

    for(campo of campos){

        if(campo.type == 'checkbox' || campo.type == 'radio'){
            if(campo.checked == true){

                var resposta = {
                    id: campo.id,
                    value: campo.value,
                };

                formulario.values.push(resposta);
            }
        }else if(campo.type == 'file'){

            var arquivo = await uploadFile(`${campo.id}`);

            var resposta = {
                id: campo.id,
                value: arquivo,
            };

            formulario.values.push(resposta);
        }else{
            var resposta = {
                id: campo.id,
                value: campo.value,
            };
            formulario.values.push(resposta);
        }


    }

    var meta = {
        title: "Novo Card",
        history: {
            drops: [],
            responsables: [],
            actions: []
        },
        tasks: [],
        tags: [],
        form: formulario
    }

    if(rastreador != ""){
    
        var tag = {
            color: "#335fff",
            id:  newId(), 
            name: rastreador
        }

        meta.tags.push(tag);

    }

    var card = {
        title: "Novo Card",
        pipe_id: pipe_id,
        step_id: step_id,
        meta: meta,
        organization_id: doc.organization_id,
    }
    {{!-- console.log(card) --}}
    criarCard(card);
};

async function criarCard(card) {

    await axios.post(`/api/card/public/create`, {
        title: card.title,
        pipe_id: card.pipe_id,
        meta: card.meta,
        step_id: card.step_id,
        organization_id: doc.organization_id,
    }).then((resultado)=>{

        console.log(resultado)

        if(resultado.status == 200){

            let metadata = {
                to: {
                    step: card.step_id,
                    card: resultado.data.id,
                    pipe: card.pipe_id,
                },
                usuario: "",
                card: resultado.data,
                organization_id: doc.organization_id
            }

            
            const socket = io.connect();

            socket.emit("move_card", metadata, (card) => {
                console.log('Aviso enviado!');
            }); 
           

            var campos = ""; 
            campos += `<h1>Obrigado!</h1>`;
            campos += `<p>Os dados foram enviados.</p>`;
            $('.conteudo').html(campos);
        }

    }).catch((error)=>{
        alert(error)
    })

}

function getBase64(file) {

    return new Promise((resolve, reject) => {

        var reader = new FileReader();
        reader.readAsDataURL(file);
        reader.onload = function() {
            // // console.log(reader.result);
            return resolve(reader.result);
        };
        reader.onerror = function(error) {
            // console.log('Error: ', error);
            return reject('erro');
        };
    })
}

async function uploadFile(fileSelector) {


    var id = newId();
    var img = document.getElementById(fileSelector).files[0];
    var base = await getBase64(img);

    var ty = img.type.split('/');
    var tipo = ty[0];
    var form = ty[1];

    if (!meta.anexos) {
        meta.anexos = [];
    }

    if (form == 'vnd.openxmlformats-officedocument.spreadsheetml.sheet') {
        var form = 'xls';
    }

    if (form == 'svg+xml') {
        var form = 'svg';
    }

    var anexo = {
        id: id,
        type: tipo,
        format: form,
        name: img.name
    }

    var arquivo = "";

    if (anexo.format != undefined) {
        var formato = anexo.format.substr(0, 4);
    } else {
        var formato = 'desc';
    }

    var nomeArquivo = id + '.' + formato;

    var nome = await axios.post(`/api/upload/form`, {
        name: nomeArquivo,
        base64: base,
    })

    if (nome.status == 200) {

        return nome.data;

    } else {
        alert(nome.data);

        return "";
    }

}

